<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://jakarta.ee/xml/ns/jakartaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://jakarta.ee/xml/ns/jakartaee
         http://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd"
         version="5.0">

    <display-name>VoyageVoiture</display-name>

    <!-- Servlet pour les Routes Nationales -->
    <servlet>
        <servlet-name>RNServlet</servlet-name>
        <servlet-class>servlet.RNServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>RNServlet</servlet-name>
        <url-pattern>/rn</url-pattern>
    </servlet-mapping>

    <!-- Session config -->
    <session-config>
        <tracking-mode>COOKIE</tracking-mode>
    </session-config>

</web-app>
